@prefix : <http://www.semanticweb.org/formation/ontologies/2022/2/untitled-ontology-20#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/formation/ontologies/2022/2/untitled-ontology-20> .

<http://www.semanticweb.org/formation/ontologies/2022/2/untitled-ontology-20> rdf:type owl:Ontology ;
	owl:imports <http://data.sparna.fr/ontologies/sparnatural-config-core> , <http://data.sparna.fr/ontologies/sparnatural-config-datasources> .


#################################################################
#    Object Properties
#################################################################

:Contact a owl:ObjectProperty ;
	rdfs:subPropertyOf <http://data.sparna.fr/ontologies/sparnatural-config-core#ListProperty> ;
	rdfs:domain :Renseignement;
	rdfs:range :Ville ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "nav:aPourLieuDEffet" ;
	<http://data.sparna.fr/ontologies/sparnatural-config-datasources#datasource> :labelVille ;
    rdfs:label "Contact"@fr .

#################################################################
#    Classes
#################################################################

# Renseignement
:Renseignement a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Renseignement>.\n?this nav:aPourLieuDEffet ?lieuDEffet" ;
	rdfs:label "Renseignement"@fr .

# Chenal
:Chenal a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Chenal>" ;
    rdfs:label "Chenal"@fr .

# Large
:Large a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Large>" ;
    rdfs:label "Large"@fr .

# Littoral
:Littoral a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Littoral>" ;
    rdfs:label "Littoral"@fr .

# Secteur
:Secteur a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Secteur>" ;
    rdfs:label "Secteur"@fr .

# Zone de pilotage
:ZoneDePilotage a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#ZoneDePilotage>" ;
    rdfs:label "Zone de pilotage"@fr .

# Terre
:Terre a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Terre>" ;
    rdfs:label "Terre"@fr .

# Interieur des terres
:InterieurDesTerres a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#InterieurDesTerres>" ;
    rdfs:label "InterieurDesTerres"@fr .

# CÃ´te
:Cote a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#Cote>" ;
    rdfs:label "Cote"@fr .

# Lieu de Stationnement
:LieuDeStationnement a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#EntiteGeographique>.\n?this nav:aPourNature ?nature.\n?nature a nav:TypeDeLieuDeStationnement" ;
    rdfs:label "LieuDeStationnement"@fr .

# Ville
:Ville a owl:Class ;
	rdfs:subClassOf <http://data.sparna.fr/ontologies/sparnatural-config-core#SparnaturalClass> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-core#sparqlString> "<http://data.shom.fr/def/navigation_cotiere#EntiteGeographique>;\nnav:aPourNature teg:Ville" ;
    rdfs:label "Ville"@fr .

#################################################################
#    List
#################################################################

:labelVille a owl:NamedIndividual , <http://data.sparna.fr/ontologies/sparnatural-config-datasources#SparqlDatasource> ;
	<http://data.sparna.fr/ontologies/sparnatural-config-datasources#queryString> """SELECT DISTINCT ?uri ?label
WHERE {
    ?domain a <http://data.shom.fr/def/navigation_cotiere#Renseignement> .
    ?domain <http://data.shom.fr/def/navigation_cotiere#aPourLieuDEffet> ?uri.
    ?uri <http://www.w3.org/2000/01/rdf-schema#label> ?label
}""" .