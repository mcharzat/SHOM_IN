{
  "name": "shom_in",
  "version": "0.1.0",
  "private": true,
  "description": "```\r npm install\r ```",
  "author": "",
  "scripts": {
    "postinstall": "cp .customModule/leaflet-src.js node_modules/leaflet/dist/leaflet-src.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "tests": "jest",
    "tests:unit": "jest --testPathPattern=unit",
    "tests:integration": "jest --testPathPattern=integration"
  },
  "main": "babel.config.js",
  "dependencies": {
    "@chenfengyuan/datepicker": "^1.0.10",
    "@triply/yasgui": "^4.2.21",
    "core-js": "^3.8.3",
    "file-loader": "^6.2.0",
    "jquery": "^3.6.0",
    "leaflet": "^1.7.1",
    "leaflet-draw": "^1.0.4",
    "npm": "^8.5.5",
    "vue": "^3.2.13",
    "vue-sweetalert2": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-loader": "^6.2.0",
    "jest": "^27.5.1"
  },
  "eslintConfig": {
    "globals": {
      "L": "readonly"
    },
    "root": true,
    "env": {
      "node": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "collectCoverage": false,
    "moduleNameMapper": {
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|ttl)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-picture-input)"
    ],
    "preset": "@vue/cli-plugin-unit-jest",
    "testMatch": [
      "**/tests/integration/**/*.inte.spec.[jt]s?(x)",
      "**/tests/unit/**/*.spec.[jt]s?(x)",
      "**/__tests__/*.[jt]s?(x)"
    ]
  },
  "bugs": {
    "url": "https://github.com/mcharzat/SHOM_IN/issues"
  },
  "homepage": "https://github.com/mcharzat/SHOM_IN#readme",
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcharzat/SHOM_IN.git"
  }
}
